{
  "name": "lawtext",
  "version": "0.0.1",
  "description": "Plain text format for Japanese law",
  "main": "lib/lawtext.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "watch": "webpack -w --colors --config ./webpack.config.dev.js",
    "watch:prod": "webpack -w --colors --config ./webpack.config.prod.js",
    "build": "webpack --colors --config ./webpack.config.dev.ts",
    "build:prod": "webpack --colors --config ./webpack.config.prod.ts",
    "test": "node --max_old_space_size=2048 -r ts-node/register ./test/prepare_test.ts && mocha -r ts-node/register test/**/*.ts"
  },
  "bin": "lib/lawtext.js",
