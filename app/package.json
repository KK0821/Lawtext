{
    "name": "lawtext-app",
    "version": "0.0.1",
    "description": "",
    "homepage": "https://github.com/yamachig/lawtext#readme",
    "bugs": {
        "url": "https://github.com/yamachig/lawtext/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yamachig/lawtext.git"
    },
    "license": "MIT",
    "author": "yamachi",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "ts-node": "node -r ts-node/register -r tsconfig-paths/register --trace-warnings",
        "build": "npx webpack --mode development --color --config ./webpack-configs/localClient.ts && npx webpack --mode development --color --config ./webpack-configs/client.ts",
        "build:prod": "npx webpack --mode production --color --config ./webpack-configs/localClient.ts && npx webpack --mode production --color --config ./webpack-configs/client.ts",
        "build:test": "npx webpack --mode development --color --config ./webpack-configs/test.ts",
        "build:bin": "npx webpack --mode development --color --config ./webpack-configs/bin.ts",
        "serve": "npx webpack serve --env DEV_SERVER=1 --mode development --color --config ./webpack-configs/client.ts",
        "serve:prod": "npx webpack serve --env DEV_SERVER=1 --mode production --color --config ./webpack-configs/client.ts",
        "test": "npm run build:test && mocha --delay --timeout 60000 --reporter mochawesome ./dist-test/bundle.js",
        "watch": "npx webpack --watch --mode development --color --config ./webpack-configs/client.ts",
        "query-docs": "npx --trace-deprecation ts-node src/query-docs/index.ts --target-dir \"dist-dev/query-docs\"",
        "query-docs:prod": "npx --trace-deprecation ts-node src/query-docs/index.ts --target-dir \"dist-prod/query-docs\""
    },
    "devDependencies": {
        "@types/bootstrap": "^5.2.10",
        "@types/chai": "^4.3.16",
        "@types/file-saver": "^2.0.7",
        "@types/fs-extra": "^11.0.4",
        "@types/js-levenshtein": "^1.1.3",
        "@types/mocha": "^10.0.7",
        "@types/node": "^20.14.8",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@types/react-router-dom": "^5.3.3",
        "@types/source-map-support": "^0.5.10",
        "@types/styled-components": "^5.1.34",
        "@types/webpack-node-externals": "^3.0.4",
        "@types/yargs": "^17.0.32",
        "autoprefixer": "^10.4.19",
        "chai": "^4.4.1",
        "css-loader": "^7.1.2",
        "css-minimizer-webpack-plugin": "^7.0.0",
        "dotenv": "^16.4.5",
        "eslint": "^8.57.0",
        "eslint-plugin-react": "^7.34.3",
        "eslint-plugin-react-hooks": "^4.6.2",
        "html-loader": "^5.0.0",
        "html-webpack-plugin": "^5.6.0",
        "mini-css-extract-plugin": "^2.9.0",
        "mocha": "^10.4.0",
        "mochawesome": "^7.1.3",
        "path-browserify": "^1.0.1",
        "postcss-loader": "^8.1.1",
        "raw-loader": "^4.0.2",
        "sass-loader": "^14.2.1",
        "source-map-support": "^0.5.21",
        "style-loader": "^4.0.0",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typedoc": "^0.26.1",
        "typescript": "^5.5.2",
        "typescript-eslint": "^7.13.1",
        "webpack": "^5.92.1",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.4",
        "webpack-node-externals": "^3.0.0",
        "yargs": "^17.7.2"
    },
    "dependencies": {
        "@popperjs/core": "^2.11.8",
        "bootstrap": "^5.3.3",
        "buffer": "^6.0.3",
        "file-saver": "^2.0.5",
        "fs-extra": "^11.2.0",
        "js-levenshtein": "^1.1.6",
        "lawtext": "file:../core",
        "node-fetch": "^3.3.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.23.1",
        "styled-components": "^6.1.11"
    }
}
