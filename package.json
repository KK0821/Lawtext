{
  "name": "lawtext",
  "version": "0.0.3",
  "description": "Plain text format for Japanese law",
  "homepage": "https://github.com/yamachig/lawtext#readme",
  "bugs": {
    "url": "https://github.com/yamachig/lawtext/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yamachig/lawtext.git"
  },
  "license": "MIT",
  "author": "yamachi",
  "main": "dist/lawtext.js",
  "types": "dist/lawtext.d.ts",
  "bin": "dist/lawtext.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "emitBinDeclarations": "tsc -p bin/build/tsconfig.json",
    "build": "node --trace-deprecation --trace-warnings bin/build/ && tsc",
    "watch": "node --trace-deprecation --trace-warnings bin/build/ && tsc --watch",
    "prepare": "npm run build",
    "test": "node --max_old_space_size=2048 ./dist/test/prepare_test.js && npx mocha"
  },
  "dependencies": {
    "@xmldom/xmldom": "^0.8.0",
    "argparse": "^2.0.1",
    "browserfs": "^1.4.3",
    "cli-progress": "^3.9.1",
    "fs-extra": "^10.0.0",
    "generic-parser": "^0.1.1",
    "hash.js": "^1.1.7",
    "js-levenshtein": "^1.1.6",
    "jszip": "^3.7.1",
    "lodash": "^4.17.21",
    "node-fetch": "^3.1.0",
    "node-stream-zip": "^1.15.0",
    "nunjucks": "^3.2.3",
    "pre-build-webpack": "^0.1.0",
    "prettify-xml": "^1.2.0",
    "source-map-loader": "^3.0.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0",
    "xpath": "^0.0.32",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/argparse": "^2.0.10",
    "@types/chai": "^4.3.0",
    "@types/cli-progress": "^3.9.2",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.4",
    "@types/node-fetch": "^3.0.2",
    "@types/nunjucks": "^3.2.1",
    "@types/pegjs": "^0.10.3",
    "@types/webpack": "^5.28.0",
    "@types/webpack-node-externals": "^2.5.3",
    "@types/xmldom": "^0.1.31",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-tsdoc": "^0.2.14",
    "mocha": "^9.1.3"
  }
}
