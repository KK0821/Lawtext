{
  "name": "lawtext",
  "version": "0.0.3",
  "description": "Plain text format for Japanese law",
  "homepage": "https://github.com/yamachig/lawtext#readme",
  "bugs": {
    "url": "https://github.com/yamachig/lawtext/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yamachig/lawtext.git"
  },
  "license": "MIT",
  "author": "yamachi",
  "main": "dist/lawtext.js",
  "types": "dist/lawtext.d.ts",
  "bin": "dist/lawtext.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "ts-node": "node -r ts-node/register -r tsconfig-paths/register --trace-warnings",
    "emitBinDeclarations": "tsc -p bin/build/tsconfig.json",
    "build": "node bin/build/ && tsc",
    "prepare": "npm run build",
    "test": "node --max_old_space_size=2048 -r ts-node/register -r tsconfig-paths/register ./test/prepare_test.ts && mocha --exit --timeout 10000 -r ts-node/register -r tsconfig-paths/register test/**/*.ts"
  },
  "dependencies": {
    "argparse": "^2.0.1",
    "browserfs": "^1.4.3",
    "cli-progress": "^3.9.0",
    "fs-extra": "^9.1.0",
    "generic-parser": "^0.1.0",
    "hash.js": "^1.1.7",
    "js-levenshtein": "^1.1.6",
    "jszip": "^3.6.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "node-stream-zip": "^1.13.2",
    "nunjucks": "^3.2.3",
    "pre-build-webpack": "^0.1.0",
    "prettify-xml": "^1.2.0",
    "source-map-loader": "^2.0.1",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "ts-pegjs": "^0.3.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2",
    "xmldom": "^0.5.0",
    "xpath": "^0.0.32",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/argparse": "^2.0.5",
    "@types/chai": "^4.2.15",
    "@types/cli-progress": "^3.9.1",
    "@types/fs-extra": "^9.0.8",
    "@types/jszip": "^3.4.1",
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.35",
    "@types/node-fetch": "^2.5.8",
    "@types/nunjucks": "^3.1.4",
    "@types/pegjs": "^0.10.2",
    "@types/webpack": "^4.41.26",
    "@types/webpack-node-externals": "^2.5.0",
    "@types/xmldom": "^0.1.30",
    "@types/yargs": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "chai": "^4.3.4",
    "eslint": "^7.22.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-tsdoc": "^0.2.11",
    "mocha": "^8.3.2"
  }
}
